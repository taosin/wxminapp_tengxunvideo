.container{
	padding: 0;
	font-family: '微软雅黑';
}
.userinfo{
	margin-top: 20px;
	width: 60px;
	text-align: center;
	height: 100px;
}
.userinfo-avatar{
	width: 50px;
	height: 50px;
	border-radius: 50%;
	margin: 0 auto;
	margin-bottom: 10px;
}
.userinfo-nickname{
	font-size: 14px;
	font-weight: 700;
}
.userinfo-vip{
	border-top: 1px solid #b8b8b8; 
	height: 60px;
	padding-left: 30px;
	width: 100%;
	padding-top: 20px;
}
.vip-right{
	width: 90%;
}
.icon{
	width: 24px;
	height: 24px;
	float: left;
	margin-right: 10px;
	margin-top: 6px;
}
.vip-info{
	font-size: 14px;
	clear: both;
}
.vip-desc{
	font-size: 12px;
	color: #b8b8b8;
	clear: both;
}
.userinfo-tag{
	height: 15px;
	width: 100%;
	background: #f5f7f9;
}
.watch-history{
	padding-top: 10px;
	width: 100%;
	padding-left: 30px;
}
.text-history{
	text-align: left;
	float: left;
}